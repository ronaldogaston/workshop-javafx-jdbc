<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="444.0" prefWidth="816.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ControllerListaDepartamento">
   <children>
      <Label text="Cadastro de Departamento">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
         <padding>
            <Insets left="5.0" top="5.0" />
         </padding>
      </Label>
      <ToolBar prefHeight="40.0" prefWidth="200.0">
        <items>
          <Button fx:id="btNew" mnemonicParsing="false" onAction="#onBtNewAction" text="Novo" />
        </items>
      </ToolBar>
      <TableView fx:id="tableViewDepartamento" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="colunaId" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="colunaNome" prefWidth="75.0" text="Nome" />
            <TableColumn fx:id="colunaEditar" prefWidth="75.0" />
        </columns>
      </TableView>
   </children>
</VBox>
